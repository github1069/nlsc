<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.etc.data.mapper.TbProcLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.etc.data.entity.TbProcLog">
        <id column="id" property="id" />
        <result column="proc_name" property="procName" />
        <result column="statu" property="statu" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="cost_time" property="costTime" />
        <result column="error_code" property="errorCode" />
        <result column="error_msg" property="errorMsg" />
        <result column="step" property="step" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, proc_name, statu, start_time, end_time, cost_time, error_code, error_msg, step
    </sql>

</mapper>
